{
    "launchers": {
        "Node": {
            "command": "node tests.js",
            "protocol": "tap"
        },
        "NodePlain": {
            "command": "node tests.js"
        }
    },
    "framework": "tap",
    "src_files": [
        "*.js",
        "solutions/*.js"
    ],
    "serve_files": [
        "bundle.js"
    ],
    "before_tests": "browserify tests.js -o bundle.js",
    "on_exit": "rm bundle.js",
    "launch_in_dev": ["Node"]
}